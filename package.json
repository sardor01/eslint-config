{
  "name": "@sarast/eslint-config",
  "version": "2.2.1",
  "packageManager": "pnpm@10.9.0+sha256.ea53ab747ac8b7921de63b42eab1adf021e1fe6ded05f62f00885cc33a46118d",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sardor01/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/sardor01/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sardor01/eslint-config.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "eslint-config-inspector --config eslint-inspector.config.ts",
    "build": "pnpm run build:typegen && tsup",
    "build:typegen": "tsx scripts/typegen.ts",
    "build:inspector": "eslint-config-inspector build --config eslint-inspector.config.ts",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^9.20.0"
  },
  "dependencies": {
    "@eslint/js": "9.25.1",
    "@eslint/markdown": "6.4.0",
    "@types/eslint": "9.6.1",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-antfu": "3.1.1",
    "eslint-plugin-command": "3.2.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import-x": "4.10.6",
    "eslint-plugin-jsdoc": "50.6.9",
    "eslint-plugin-jsonc": "2.20.0",
    "eslint-plugin-n": "17.17.0",
    "eslint-plugin-perfectionist": "4.12.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-regexp": "2.7.0",
    "eslint-plugin-unicorn": "58.0.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-vue": "10.0.0",
    "eslint-plugin-yml": "1.17.0",
    "globals": "16.0.0",
    "jsonc-eslint-parser": "2.4.0",
    "local-pkg": "1.1.1",
    "prettier": "3.5.3",
    "typescript-eslint": "8.31.0",
    "vue-eslint-parser": "10.1.3",
    "yaml-eslint-parser": "1.3.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "1.0.2",
    "@sarast/prettier-config": "1.1.0",
    "@types/node": "22.14.1",
    "bumpp": "10.1.0",
    "eslint": "9.25.1",
    "eslint-typegen": "2.1.0",
    "importx": "0.5.2",
    "picocolors": "1.1.1",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ],
    "overrides": {
      "is-core-module": "npm:@no-shims/is-core-module"
    }
  },
  "prettier": "@sarast/prettier-config"
}
