{
  "name": "@sarast/eslint-config",
  "version": "1.7.0",
  "packageManager": "pnpm@9.15.4+sha256.9bee59c7313a216722c079c1e22160dea7f88df4e0c3450b1d7b01b882336c6a",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sardor01/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/sardor01/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sardor01/eslint-config.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "eslint-config-inspector --config eslint-inspector.config.ts",
    "build": "pnpm run build:typegen && tsup",
    "build:typegen": "tsx scripts/typegen.ts",
    "build:inspector": "eslint-config-inspector build --config eslint-inspector.config.ts",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^9.10.0"
  },
  "dependencies": {
    "@eslint/js": "9.18.0",
    "@eslint/markdown": "6.2.1",
    "@types/eslint": "9.6.1",
    "@unocss/eslint-plugin": "65.4.0",
    "eslint-config-flat-gitignore": "1.0.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-antfu": "2.7.0",
    "eslint-plugin-command": "2.1.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import-x": "4.6.1",
    "eslint-plugin-jsdoc": "50.6.1",
    "eslint-plugin-jsonc": "2.18.2",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-perfectionist": "4.6.0",
    "eslint-plugin-prettier": "5.2.2",
    "eslint-plugin-regexp": "2.7.0",
    "eslint-plugin-unicorn": "56.0.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-vue": "9.32.0",
    "eslint-plugin-yml": "1.16.0",
    "globals": "15.14.0",
    "jsonc-eslint-parser": "2.4.0",
    "local-pkg": "1.0.0",
    "prettier": "3.4.2",
    "typescript-eslint": "8.20.0",
    "vue-eslint-parser": "9.4.3",
    "yaml-eslint-parser": "1.2.3"
  },
  "devDependencies": {
    "@eslint/config-inspector": "1.0.0",
    "@sarast/prettier-config": "1.1.0",
    "@types/node": "22.10.6",
    "bumpp": "9.10.1",
    "eslint": "9.18.0",
    "eslint-typegen": "1.0.0",
    "importx": "0.5.1",
    "picocolors": "1.1.1",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@no-shims/is-core-module"
    }
  },
  "prettier": "@sarast/prettier-config"
}
