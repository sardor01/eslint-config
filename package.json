{
  "name": "@sarast/eslint-config",
  "version": "4.0.0",
  "packageManager": "pnpm@10.10.0+sha256.fa0f513aa8191764d2b6b432420788c270f07b4f999099b65bb2010eec702a30",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sardor01/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/sardor01/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sardor01/eslint-config.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "eslint-config-inspector --config eslint-inspector.config.ts",
    "build": "pnpm run build:typegen && tsup",
    "build:typegen": "tsx scripts/typegen.ts",
    "build:inspector": "eslint-config-inspector build --config eslint-inspector.config.ts",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^9.20.0"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint/js": "9.26.0",
    "@eslint/markdown": "6.4.0",
    "@types/eslint": "9.6.1",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-flat-config-utils": "2.0.1",
    "eslint-plugin-antfu": "3.1.1",
    "eslint-plugin-command": "3.2.0",
    "eslint-plugin-import-x": "4.11.0",
    "eslint-plugin-jsdoc": "50.6.11",
    "eslint-plugin-jsonc": "2.20.0",
    "eslint-plugin-n": "17.18.0",
    "eslint-plugin-perfectionist": "4.12.3",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-regexp": "2.7.0",
    "eslint-plugin-unicorn": "59.0.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-vue": "10.1.0",
    "eslint-plugin-yml": "1.18.0",
    "globals": "16.1.0",
    "jsonc-eslint-parser": "2.4.0",
    "local-pkg": "1.1.1",
    "prettier": "3.5.3",
    "typescript-eslint": "8.32.0",
    "vue-eslint-parser": "10.1.3",
    "yaml-eslint-parser": "1.3.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "1.0.2",
    "@sarast/prettier-config": "1.1.0",
    "@types/node": "22.15.17",
    "bumpp": "10.1.0",
    "eslint": "9.26.0",
    "eslint-typegen": "2.1.0",
    "importx": "0.5.2",
    "picocolors": "1.1.1",
    "tsup": "8.4.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ],
    "overrides": {
      "is-core-module": "npm:@no-shims/is-core-module"
    }
  },
  "prettier": "@sarast/prettier-config"
}
