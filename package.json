{
  "name": "@sarast/eslint-config",
  "version": "5.0.1",
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sardor01/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/sardor01/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sardor01/eslint-config.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "eslint-config-inspector --config eslint-inspector.config.ts",
    "build": "pnpm run build:typegen && tsup",
    "build:typegen": "tsx scripts/typegen.ts",
    "build:inspector": "eslint-config-inspector build --config eslint-inspector.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lf": "pnpm lint:fix && pnpm format:fix",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^9.28.0"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint/js": "9.29.0",
    "@eslint/markdown": "6.6.0",
    "@types/eslint": "9.6.1",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-flat-config-utils": "2.1.0",
    "eslint-plugin-antfu": "3.1.1",
    "eslint-plugin-command": "3.3.0",
    "eslint-plugin-import-x": "4.15.2",
    "eslint-plugin-jsdoc": "51.1.2",
    "eslint-plugin-jsonc": "2.20.1",
    "eslint-plugin-n": "17.20.0",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-prettier": "5.5.0",
    "eslint-plugin-regexp": "2.9.0",
    "eslint-plugin-unicorn": "59.0.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-vue": "10.2.0",
    "eslint-plugin-yml": "1.18.0",
    "globals": "16.2.0",
    "jsonc-eslint-parser": "2.4.0",
    "local-pkg": "1.1.1",
    "typescript-eslint": "8.34.1",
    "vue-eslint-parser": "10.1.3",
    "yaml-eslint-parser": "1.3.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "1.1.0",
    "@sarast/prettier-config": "1.2.1",
    "@types/node": "24.0.3",
    "bumpp": "10.2.0",
    "eslint": "9.29.0",
    "eslint-typegen": "2.2.0",
    "importx": "0.5.2",
    "picocolors": "1.1.1",
    "prettier": "3.5.3",
    "tsup": "8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=22"
  },
  "prettier": "@sarast/prettier-config"
}
