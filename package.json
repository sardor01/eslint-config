{
  "name": "@sarast/eslint-config",
  "version": "5.4.0",
  "packageManager": "pnpm@10.18.0+sha512.e804f889f1cecc40d572db084eec3e4881739f8dec69c0ff10d2d1beff9a4e309383ba27b5b750059d7f4c149535b6cd0d2cb1ed3aeb739239a4284a68f40cfa",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sardor01/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/sardor01/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sardor01/eslint-config.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "eslint-config-inspector --config eslint-inspector.config.ts",
    "build": "pnpm run build:typegen && tsup",
    "build:typegen": "tsx scripts/typegen.ts",
    "build:inspector": "eslint-config-inspector build --config eslint-inspector.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lf": "pnpm lint:fix && pnpm format:fix",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^9.28.0"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint/js": "9.37.0",
    "@eslint/markdown": "7.3.0",
    "@types/eslint": "9.6.1",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-flat-config-utils": "2.1.4",
    "eslint-plugin-antfu": "3.1.1",
    "eslint-plugin-command": "3.3.1",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsdoc": "60.8.0",
    "eslint-plugin-jsonc": "2.21.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-regexp": "2.10.0",
    "eslint-plugin-unicorn": "61.0.2",
    "eslint-plugin-unused-imports": "4.2.0",
    "eslint-plugin-vue": "10.5.0",
    "eslint-plugin-yml": "1.19.0",
    "globals": "16.4.0",
    "jsonc-eslint-parser": "2.4.1",
    "local-pkg": "1.1.2",
    "typescript-eslint": "8.45.0",
    "vue-eslint-parser": "10.2.0",
    "yaml-eslint-parser": "1.3.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "1.3.0",
    "@sarast/prettier-config": "1.2.1",
    "@types/node": "24.6.2",
    "bumpp": "10.2.3",
    "eslint": "9.37.0",
    "eslint-typegen": "2.3.0",
    "importx": "0.5.2",
    "picocolors": "1.1.1",
    "prettier": "3.6.2",
    "tsup": "8.5.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=22"
  },
  "prettier": "@sarast/prettier-config"
}
